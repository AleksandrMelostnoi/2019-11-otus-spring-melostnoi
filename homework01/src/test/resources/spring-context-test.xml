<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="inputConsoleService" class="ru.otus.homework01.service.InputConsoleServiceImpl">
    </bean>

    <bean id="quizDao" class="ru.otus.homework01.dao.QuizDaoImpl">
        <constructor-arg name="PATH" value="questions.csv"/>
    </bean>

    <bean id="quizService" class="ru.otus.homework01.service.QuizServiceImpl">
        <constructor-arg name="inputConsoleService" ref="inputConsoleService"/>
        <constructor-arg name="quizDao" ref="quizDao"/>
    </bean>

</beans>
